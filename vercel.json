{
  "env": {
    "DATABASE_URL": "@database-url",
    "GITHUB_TOKEN": "@github-token",
    "NEXTAUTH_SECRET": "@nextauth-secret"
  },
  "functions": {
    "app/api/**.js": {
      "maxDuration": 10
    }
  },
  "crons": [
    {
      "path": "/api/cron/crawl-data",
      "schedule": "0 6 * * *"
    }
  ]
}
